# PostHog MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: Your PostHog instance URL
POSTHOG_HOST=https://posthog.myteam.network

# Required: Personal API key for management operations
# Get from: Account Settings → Personal API Keys
# Format: phx_...
POSTHOG_API_KEY=phx_your_personal_api_key_here

# Optional: Project API key for event capture
# Get from: Project Settings → API Keys  
# Format: phc_...
# Leave empty if you don't need event capture
POSTHOG_PROJECT_API_KEY=phc_your_project_api_key_here

# Optional: Default project ID
POSTHOG_PROJECT_ID=1

# Optional: MCP Server Configuration
MCP_SERVER_NAME=posthog-mcp
MCP_SERVER_VERSION=1.0.0

# Optional: Rate limiting
RATE_LIMIT_MAX_REQUESTS=200

# Optional: Caching
CACHE_ENABLED=false
CACHE_TTL=300