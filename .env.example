# PostHog MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: Your PostHog instance URL
POSTHOG_HOST=https://posthog.myteam.network

# Required: Your personal API key from PostHog
# Get one from: https://posthog.myteam.network/project/settings#project-api-keys
POSTHOG_API_KEY=phx_your_personal_api_key_here

# Optional: Default project ID
POSTHOG_PROJECT_ID=1

# Optional: MCP Server Configuration
MCP_SERVER_NAME=posthog-mcp
MCP_SERVER_VERSION=1.0.0

# Optional: Rate limiting
RATE_LIMIT_MAX_REQUESTS=200

# Optional: Caching
CACHE_ENABLED=false
CACHE_TTL=300